    <link rel="shortcut icon" href="~/AdminTemplate/assets/images/favicon.png">

    <!-- BEGIN CSS -->
    <link href="~/AdminTemplate/assets/plugins/bootstrap/bootstrap5/css/bootstrap.rtl.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/metisMenu/dist/metisMenu.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/simple-line-icons/css/simple-line-icons.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/font-awesome/css/all.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/switchery/dist/switchery.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/sweetalert2/dist/sweetalert2.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/paper-ripple/dist/paper-ripple.min.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/iCheck/skins/square/_all.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/css/style.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/css/colors.css" rel="stylesheet">
    <!-- END CSS -->
    <!-- BEGIN PAGE CSS -->
    <link href="~/AdminTemplate/assets/plugins/morris.js/morris.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/plugins/persian-datepicker/css/persian-datepicker.css" rel="stylesheet">
    <link href="~/AdminTemplate/assets/css/effects.css" rel="stylesheet">

    <link href="~/AdminTemplate/assets/plugins/data-table/DataTables-1.10.16/css/jquery.dataTables.css" rel="stylesheet">

    <!-- END PAGE CSS -->
</head>
<body class="active-ripple theme-darkpurple fix-header sidebar-extra bg-1">


    <!-- BEGIN HEADER -->

    @await Html.PartialAsync("_Header")
   
    <!-- END HEADER -->
    <!-- BEGIN WRAPPER -->
    <div id="wrapper">

        <!-- BEGIN SIDEBAR -->
        @await Html.PartialAsync("_Sidebar")
        <!-- END SIDEBAR -->
        <!-- BEGIN PAGE CONTENT -->
        <div id="page-content">
            <div id="inner-content">
                <div class="row">
                    <!-- BEGIN BREADCRUMB -->
                    <div class="col-md-12">
                        <div class="breadcrumb-box shadow">
                            <ul class="breadcrumb">
                                <li><a href="dashboard.html">@ViewData["Title"]</a></li>
                            </ul>
                            <div class="breadcrumb-left">
                                @DateTime.Now.ToPersianString("dddd, dd MMMM,yyyy")
                                <i class="icon-calendar"></i>
                            </div><!-- /.breadcrumb-left -->
                        </div><!-- /.breadcrumb-box -->
                    </div><!-- /.col-md-12 -->
                    <!-- END BREADCRUMB -->

                    <div class="col-md-12">
                        <div class="row">
                            
                                @RenderBody()

                           

                        </div><!-- /.row -->
                    </div><!-- /.col-md-12 -->
                </div><!-- /.row -->
            </div><!-- /#inner-content -->
        </div><!-- /#page-content -->
        <!-- END PAGE CONTENT -->

    </div><!-- /#wrapper -->
    <!-- END WRAPPER -->
    @await Html.PartialAsync("_Footer")
    <!-- BEGIN SETTING -->
    @await Html.PartialAsync("_Setting")
    <!-- END SETTING -->
    <!-- BEGIN CODE MODAL -->
    <div class="modal fade" id="code-modal" role="dialog" tabindex="-1">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="btn btn-default btn-round btn-icon float-start" id="btn-copy"><i class="far fa-copy"></i></button>
                    <button type="button" class="close" data-bs-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">کپی کردن کدها</h4>
                </div>
                <div class="modal-body"></div>
            </div> <!-- /.modal-content -->
        </div> <!-- /.modal-dialog -->
    </div> <!-- /.modal -->
    <!-- END CODE MODAL -->
    <!-- BEGIN JS -->
    <script src="~/AdminTemplate/assets/plugins/jquery/dist/jquery-3.6.1.min.js"></script>
    <script src="~/AdminTemplate/assets/plugins/bootstrap/bootstrap5/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminTemplate/assets/plugins/metisMenu/dist/metisMenu.min.js"></script>
    <script src="~/AdminTemplate/assets/plugins/paper-ripple/dist/PaperRipple.min.js"></script>
