    <script src="~/AdminTemplate/assets/plugins/sweetalert2/dist/sweetalert2.min.js"></script>
    <script src="~/AdminTemplate/assets/plugins/screenfull/dist/screenfull.min.js"></script>
    <script src="~/AdminTemplate/assets/plugins/iCheck/icheck.min.js"></script>
    <script src="~/AdminTemplate/assets/plugins/switchery/dist/switchery.js"></script>
    <script src="~/AdminTemplate/assets/js/core.js"></script>

    <!-- BEGIN PAGE JAVASCRIPT -->

    <!-- END PAGE JAVASCRIPT -->
    @await RenderSectionAsync("Scripts", required: false)
</body>

